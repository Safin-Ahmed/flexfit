{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "avatar": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "workouts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::workout.workout",
      "mappedBy": "users_permissions_user"
    },
    "reminders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::reminder.reminder",
      "mappedBy": "users_permissions_user"
    },
    "point": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::point.point",
      "mappedBy": "users_permissions_user"
    },
    "user_progress": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::user-progress.user-progress",
      "mappedBy": "users_permissions_user"
    },
    "completed_workouts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::completed-workout.completed-workout",
      "mappedBy": "user"
    }
  }
}
